<robot name="dyros_manipulator3r">
    <link name="base_link">
        <visual>
        <origin xyz="0 0 0.1" rpy="0 0 0"/>
        <geometry>
            <box size="0.05 0.05 0.2"/>
        </geometry>
        </visual>
    </link>
